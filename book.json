{
    "root": "./docs",
    "title": "awesome-frontend-handbook",
    "structure": {
      "readme": "README.md"
    },
    "plugins": [
      "-highlight",
      "search",
      "sharing",
      "livereload",
      "fontsettings",
      "prism",
      "github",
      "styles-sass-fix"
    ],
    "styles":{
        "website":"../styles/website.scss"
    },
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/lihxhit/awesome-frontend-handbook.git"
        },
        "fontsettings": {
            "theme": "white",
            "family": "serif",
            "size":  1
        },
        "theme-default":{
            "showLevel":true
        }

    }
}