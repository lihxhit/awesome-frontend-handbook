{
    "root": "./docs",
    "title": "awesome-frontend-handbook",
    "structure": {
      "readme": "README.md"
    },
    "plugins": [
      "-highlight",
      "-search",
      "sharing",
      "livereload",
      "fontsettings",
      "prism",
      "github",
      "disqus",
      "anchors",
      "theme-gestalt", "-theme-default", "styles-sass-fix"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/lihxhit/awesome-frontend-handbook.git"
        },
        "fontsettings": {
            "theme": "white",
            "family": "serif",
            "size":  1
        },
        "disqus": {
            "shortName": "XXXXXXX"
        }
    }
}